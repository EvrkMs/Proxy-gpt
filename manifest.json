{
    "manifest_version": 3,
    "name": "Proxy Manager",
    "version": "1.0",
    "permissions": ["storage", "proxy", "declarativeNetRequest"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  